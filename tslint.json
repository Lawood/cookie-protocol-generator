{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": true,
        "no-console": false,
        "max-line-length": [
          true,
          150
        ],
        "indent": [
            true,
            "spaces",
            2
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "avoid-template"
        ]
    },
    "rulesDirectory": []
}
